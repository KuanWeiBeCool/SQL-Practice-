/*
Enter your query here.
*/
SELECT X1, Y1 FROM
(
(SELECT F1.X X1, F1.Y Y1, F2.X X2, F2.Y Y2
FROM Functions F1 
INNER JOIN Functions F2 ON (F1.X = F2.Y AND F1.Y = F2.X)
WHERE F1.X != F1.Y
) 
UNION 
(SELECT F1.X X1, F1.Y Y1, F2.X X2, F2.Y Y2
FROM Functions F1 
INNER JOIN Functions F2 ON (F1.X = F2.Y AND F1.Y = F2.X)
WHERE F1.X = F1.Y
GROUP BY X1, X2
HAVING COUNT(*) > 1
)
) Result_1
WHERE X1 <= Y1
ORDER BY X1



